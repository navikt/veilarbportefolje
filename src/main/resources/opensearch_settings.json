      },
      "arbeidsliste_tittel_sortering": {
        "type": "keyword"
      },
      "arbeidsliste_tittel_lengde": {
        "type": "byte"
      },
      "fullt_navn": {
        "type": "text",
        "analyzer": "fullt_navn_sok",
        "fields": {
          "raw": {
            "type": "keyword"
          }
        }
      },
      "fnr": {
        "type": "text",
