{
  "settings": {
    "index": {
      "number_of_shards": 3,
      "number_of_replicas": 2,
      "refresh_interval": "1s",
      "max_ngram_diff": 18
    },
    "analysis": {
      "filter": {
        "spesialtegnfolding": {
          "type": "asciifolding",
          "preserve_original": true
        }
      },
      "analyzer": {
        "fullt_navn_sok": {
          "tokenizer": "fullt_navn_sok_tokenizer",
          "filter": [
            "lowercase",
            "spesialtegnfolding"
          ]
        },
        "fnr_sok": {
          "tokenizer": "fnr_sok_tokenizer"
        }
      },
      "tokenizer": {
        "fullt_navn_sok_tokenizer": {
          "type": "ngram",
          "min_gram": 2,
          "max_gram": 20
        },
        "fnr_sok_tokenizer": {
          "type": "ngram",
          "min_gram": 2,
          "max_gram": 12,
          "token_chars": [
            "digit"
          ]
        }
      }
    }
  },
  "mappings": {
    "properties": {
      "veileder_id": {
        "type": "keyword"
      },
